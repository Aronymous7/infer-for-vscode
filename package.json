{
  "name": "infer-for-vscode",
  "displayName": "Infer for VSCode",
  "description": "Static runtime analysis for Java code, powered by Facebook's Infer.",
  "version": "0.0.1",
  "publisher": "Aaron Beigelbeck",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Debuggers"
  ],
  "keywords": [
    "Infer",
    "Facebook",
    "Runtime analysis",
    "Static analysis",
    "Java"
  ],
  "activationEvents": [
    "onCommand:infer-for-vscode.runInferWebview",
    "onCommand:infer-for-vscode.enableCodeLens",
    "onCommand:infer-for-vscode.disableCodeLens"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "infer-for-vscode.runInferWebview",
        "title": "Infer - Webview",
        "category": "Webview"
      },
      {
        "command": "infer-for-vscode.enableCodeLens",
        "title": "Infer - Enable CodeLens",
        "category": "CodeLens"
      },
      {
        "command": "infer-for-vscode.disableCodeLens",
        "title": "Infer - Disable CodeLens",
        "category": "CodeLens"
      }
    ],
    "configuration": {
      "properties": {
        "infer-for-vscode.enableCodeLens": {
          "type": "boolean",
          "default": false
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "sanitize-html": "^1.27.0"
  }
}
